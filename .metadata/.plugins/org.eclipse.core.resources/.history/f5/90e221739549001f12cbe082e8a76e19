package testing.com;

import java.util.Scanner;

public class TriangleClassifier {

	public static void main(String[] args) {
		
		int side1, side2, side3;
		
		System.out.println("Enter the 3 sides of triangle");
		Scanner sc = new Scanner(System.in);
		side1 = sc.nextInt();
		side2 = sc.nextInt();
		side3 = sc.nextInt();

	}

}
